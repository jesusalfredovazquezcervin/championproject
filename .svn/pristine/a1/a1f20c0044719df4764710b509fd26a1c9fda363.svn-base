<%@ page import="championweb.Zona" %>



<div class="fieldcontain ${hasErrors(bean: zonaInstance, field: 'nombreZona', 'error')} required">
	<label for="nombreZona">
		<g:message code="zona.nombreZona.label" default="Nombre Zona" />
		<span class="required-indicator">*</span>
	</label>
	<g:textField name="nombreZona" required=""  maxlength="30" value="${zonaInstance?.nombreZona}"/>
</div>

<div class="fieldcontain ${hasErrors(bean: zonaInstance, field: 'descripcion', 'error')} ">
	<label for="descripcion">
		<g:message code="zona.descripcion.label" default="Descripcion" />
		
	</label>
	<g:textField name="descripcion" value="${zonaInstance?.descripcion}"/>
</div>

