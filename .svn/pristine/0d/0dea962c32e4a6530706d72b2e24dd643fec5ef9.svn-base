package com.koomoni.dto;

import java.io.Serializable;

public class ProductoDTO implements Serializable{

	private static final long serialVersionUID = 1L;
	private String clave;
	private String descripcion;
	private String familia;
	private String claveAlterna;
	private String uni_med;
	
	//se agrega para la parte de stock
	private String claveAlmacen;
	private String descAlmacen;
	private String existencia;
	
	public String getClaveAlmacen() {
		return claveAlmacen;
	}
	public void setClaveAlmacen(String claveAlmacen) {
		this.claveAlmacen = claveAlmacen;
	}
	
	public String getDescAlmacen() {
		return descAlmacen;
	}
	public void setDescAlmacen(String descAlmacen) {
		this.descAlmacen = descAlmacen;
	}
	
	public void setExistencia(String existencia){
		this.existencia = existencia;
	}
	public String getExistencia(){
		return existencia;
	}
	
	
	public String getClave() {
		return clave;
	}
	public void setClave(String clave) {
		this.clave = clave;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public String getFamilia() {
		return familia;
	}
	public void setFamilia(String familia) {
		this.familia = familia;
	}
	public String getClaveAlterna() {
		return claveAlterna;
	}
	public void setClaveAlterna(String claveAlterna) {
		this.claveAlterna = claveAlterna;
	}
	public String getUni_med() {
		return uni_med;
	}

	public void setUni_med(String uni_med) {
		this.uni_med = uni_med;
	}
	
	
}
